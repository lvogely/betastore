<h1>Listing logins</h1>

<div class="form">
    <% @logins.each do |login| %>
 
      <%= link_to 'Show', login %>
      <%= link_to 'Edit', edit_login_path(login) %>
      <%= link_to 'Destroy', login, method: :delete, data: { confirm: 'Are you sure?' } %>
</div>
<br>

<%= link_to 'New Login', new_login_path %>
